<html>
  <head>
    <title>Home Page</title>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Click on the link to <a href="/new">add a new message</a>.</p>
    <% messages.forEach(message => { %>
      <div>
        <span><%= message.user %></span>
        <p><%= message.text %></p>
        <span><%= message.added %></span>
      </div>
    <% }) %>
  </body>
</html>
